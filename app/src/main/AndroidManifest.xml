<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fabio.scarcella.dgym">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DGym">
        <activity
            android:name=".MuscularGroups"
            android:label="@string/title_activity_muscular_groups"
            android:theme="@style/Theme.DGym.NoActionBar"></activity>
        <activity
            android:name=".ScrollingActivity"
            android:label="@string/title_activity_scrolling"
            android:theme="@style/Theme.DGym.NoActionBar" />
        <activity
            android:name=".search_ecercises"
            android:label="@string/title_activity_search_ecercises"
            android:theme="@style/Theme.DGym.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>